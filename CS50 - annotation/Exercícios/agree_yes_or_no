# include <cs50.h>
# include <stdio.h>

int main(void)
{
    // chars são caracteres individuais.
    char c = get_char("Do you agree ? ");
    // Use aspas simples quando você está comparando caracteres individuais.
    if (c == 'y' || c == 'Y')
    {
        printf("Agreed.\n");
    }

    else if ( c == 'n' || c == 'N')
    {
        printf("Not agreed.\n");
    }


}
