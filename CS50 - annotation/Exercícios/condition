// Conditions and relational operators

# include <cs50.h>
# include <stdio.h>

int main(void)
{
    // Prompt user for x
    int x = get_int("What is x ? ");

    // Prompt user for y
    int y = get_int("What is y ? ");

    // Compare x and y
    if (x < y)
    {
        printf("X is less than Y\n");
    }

    else if (x > y)
    {
        printf("X is greater than Y\n");
    }

    else
    {
        printf("X is equal to Y\n");
    }
}
